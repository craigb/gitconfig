[credential]
    helper = manager
[user]
    email = tony@codeawhile.com
    name = Tony Ferrell
[push]
    default = current
[core]
    editor = 'gvim.bat' -f
[rebase]
    autoSquash = true
[alias]
    glog = log --graph --pretty=format:'%C(cyan)%h%Creset -%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%C(yellow)%d%Creset'
    fshow = show --pretty="" --name-only
    fup = commit --fixup
    sup = commit --squash
[merge]
    conflictstyle = diff3
[status]
  showUntrackedFiles = all
